<h1>My list of lists</h1>
<div class="cards texte-white"><% @lists.each do |list| %>
<div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3))">
<%= link_to list.name, list_path(list) %></div>
<% end %></div>

<%= link_to "Create a list", new_list_path, class: "btn btn-ghost" %>
